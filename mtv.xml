<interface uri="http://talex5.github.io/mirage-trace-viewer/mtv.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface" xmlns:compile="http://zero-install.sourceforge.net/2006/namespaces/0compile">
  <name>mirage-trace-viewer</name>
  <summary>viewer for mirage-profile trace files</summary>

  <description>
Can load trace data from a file, a running Unix process or a Xen domain and save
it to a CTF file, display it in a GTK window, or generate a JavaScript viewer
for it.</description>

  <homepage>https://github.com/talex5/mirage-trace-viewer</homepage>
  <category>Development</category>

  <group license="OSI Approved :: BSD-2-Clause">
    <command name="compile" shell-command="$SRCDIR/0install/build.sh">
      <compile:implementation xmlns:compile="http://zero-install.sourceforge.net/2006/namespaces/0compile">
	<command name="run" path="bin/mirage-trace-viewer"/>
      </compile:implementation>
    </command>

    <implementation arch="*-src" compile:dup-src="true" id="sha1new=9d783aceeb10992bb341d67da9a105b03c360f3d" released="2014-12-12" version="0.1">
      <manifest-digest sha256new="NKDNFAJU6WTUHX3K57RN57BLRSWJCPVYZW66DMYGO7QLPB3ZYCKA"/>
      <archive extract="mirage-trace-viewer-0.1" href="https://github.com/talex5/mirage-trace-viewer/archive/v0.1.tar.gz" size="139553"/>
    </implementation>
  </group>
</interface>
